{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-json-modules src/index.js",
    "dev": "nodemon -r dotenv/config --experimental-json-modules src/index.js",
    "test": "node --experimental-vm-modules --experimental-json-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules --experimental-json-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules --experimental-json-modules node_modules/jest/bin/jest.js --coverage"
  },
  "author": "Dipti Yadav",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "multer": "^1.4.5-lts.1",
    "otp-generator": "^4.0.1",
    "razorpay": "^2.9.6",
    "react-helmet-async": "^2.0.5",
    "twilio": "^5.10.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^8.12.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,mjs}",
      "**/*.{test,spec}.{js,mjs}"
    ],
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.{js,mjs}",
      "!src/**/*.test.{js,mjs}",
      "!src/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  }
}
